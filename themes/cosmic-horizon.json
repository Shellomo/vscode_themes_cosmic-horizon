{
    "name": "Cosmic Horizon",
    "type": "dark",
    "colors": {
      "editor.background": "#1a1b26",
      "editor.foreground": "#d4d4d4",
      "activityBar.background": "#13141f",
      "activityBar.foreground": "#7aa2f7",
      "activityBarBadge.background": "#7aa2f7",
      "activityBarBadge.foreground": "#ffffff",
      "sideBar.background": "#16161e",
      "sideBar.foreground": "#a9b1d6",
      "sideBarTitle.foreground": "#7aa2f7",
      "statusBar.background": "#13141f",
      "statusBar.foreground": "#7aa2f7",
      "titleBar.activeBackground": "#13141f",
      "titleBar.activeForeground": "#7aa2f7",
      "tab.activeBackground": "#1a1b26",
      "tab.inactiveBackground": "#16161e",
      "tab.activeForeground": "#7aa2f7",
      "tab.inactiveForeground": "#787c99"
    },
    "tokenColors": [
      {
        "name": "Keywords",
        "scope": ["keyword", "storage.type", "storage.modifier"],
        "settings": {
          "foreground": "#9d7cd8"
        }
      },
      {
        "name": "Strings",
        "scope": ["string", "string.quoted"],
        "settings": {
          "foreground": "#9ece6a"
        }
      },
      {
        "name": "Functions",
        "scope": ["entity.name.function", "support.function"],
        "settings": {
          "foreground": "#7aa2f7"
        }
      },
      {
        "name": "Variables",
        "scope": ["variable", "variable.other"],
        "settings": {
          "foreground": "#bb9af7"
        }
      },
      {
        "name": "Constants",
        "scope": ["constant", "variable.other.constant"],
        "settings": {
          "foreground": "#ff9e64"
        }
      },
      {
        "name": "Classes",
        "scope": ["entity.name.type.class", "entity.other.inherited-class"],
        "settings": {
          "foreground": "#e0af68"
        }
      },
      {
        "name": "Comments",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#565f89",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Operators",
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#89ddff"
        }
      }
    ]
  }